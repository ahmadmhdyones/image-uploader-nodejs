{
	"name": "image-uploader-nodejs",
	"version": "0.0.1",
	"description": "NodeJS application that support upload images API and request the image with specific width and height.",
	"main": "server.js",
	"type": "commonjs",
	"scripts": {
		"start": "node .",
		"start:dev": "nodemon .",
		"start:prod": "NODE_ENV=production nodemon .",
		"debug": "npx ndb .",
		"lint": "npx eslint .",
		"lint:fix": "npx eslint . --fix",
		"format": "npx prettier \"**/*.{js,json,css,md,html}\"",
		"format:fix": "npx prettier --write \"**/*.{js,json,css,md,html}\"",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ahmadmhdyones/image-uploader-nodejs.git"
	},
	"keywords": [
		"mongodb",
		"express.js",
		"node.js"
	],
	"author": "Ahmad MHD Yones",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ahmadmhdyones/image-uploader-nodejs/issues"
	},
	"homepage": "https://github.com/ahmadmhdyones/image-uploader-nodejs#readme",
	"dependencies": {
		"colors": "^1.4.0",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-mongo-sanitize": "^2.2.0",
		"express-rate-limit": "^6.7.1",
		"helmet": "^7.0.0",
		"hpp": "^0.2.3",
		"mongoose": "^7.3.3",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"rotating-file-stream": "^3.1.0",
		"slugify": "^1.6.6",
		"validator": "^13.9.0",
		"xss-clean": "^0.1.4"
	},
	"devDependencies": {
		"eslint": "^8.44.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.0.0",
		"ndb": "^1.1.5",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.0"
	}
}
